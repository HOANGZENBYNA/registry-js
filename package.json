  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.9.1",
    "benchmark": "^2.1.4",
    "jest": "^24.0.0",
    "prettier": "^1.14.2",
    "ts-node": "^8.0.1",
